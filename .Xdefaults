Xcursor*theme: comix-opaque

!
!      X F T
!
! 
Xft.dpi:       96
Xft.antialias: true
Xft.hinting:   1
Xft.hintstyle: hintfull
Xft.rgba:      none

URxvt*font:      xft:Monospace:size=10
URxvt*scrollBar: false
!
!
!      X T E R M
!
!
xterm*termname:  xterm-256color 
xterm*internalBorder:        0
xterm*loginShell:         true
xterm*scrollBar:         false
xterm*cursorBlink:        true
!xterm*geometry:         80x26
xterm*saveLines:         65535
xterm*dynamicColors:        on
xterm*highlightSelection: true
xterm*set_titles:   	  true
xterm.urgentOnBell:       false
! Catch URL's?
xterm*charClass:     33:48,35-38:48,39:43,42-47:48,58-59:48,61:48,63-64:48,126:48
! Appearance
xterm*utf8:          2
xterm*eightBitInput: true
xterm*faceName:      Monospace:size=10

